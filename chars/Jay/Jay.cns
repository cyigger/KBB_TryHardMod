;===============================================================================
; A:
;-------------------------------------------------------------------------------
; A - 1
[Statedef 200]
type    = S ; The character is standing on the ground
movetype= A ; This state is an attack
physics = S ; The character is using standing physics
juggle  = 1 ; Adds 1 "Juggle point" if the opponent is in a juggle state. This move will not hit if the opponent's juggle points would go over the juggle limit (20)
ctrl = 0 ; The player does not have the ability to freely control their character in this state. They must use a change state built into this move
anim = 200 ; This state uses animation 200 (Which contains the hitboxes and hurtboxes of this attack)
poweradd = 0 ; Gives 0 power on hit (Power gain code is below)
sprpriority = 2 ; Sprite priority, this sprite will appear over other sprites with a lower priority

[State 0, Juggle Map]
type = MapAdd
trigger1 = movehit && enemy,gethitvar(fall)
map = "juggle"
value = 5
persistent = 0

[State 200, Damage Proration]
type = VarAdd
trigger1 = movehit ;If the Attack Hits...
v = 11 ;This variable will be the one the proration points will be added to.
ignorehitpause = 1 ;This block of code will ignore hitpause.
persistent = 0 ;This will only trigger once.
value = 5 ;...Add this number of proration points. In this case, it adds 5.

[State 200, Power Gain]
type = PowerAdd
triggerall = numhelper(51000) = 0 ;If character is not in Starburst...
triggerall = numhelper(55000) = 0 ;If character is not in Starbuster cooldown...
triggerall = numhelper(56000) = 0 ;If character is not in Power Delay cooldown...
trigger1 = movecontact = 1 ;If the move has made contact...
value = 100 ;Add this number of meter. Max is 3000.

[State 200, Woosh] 
type = PlaySnd
trigger1 = AnimElem = 1
value = S1,1
volumescale = 50

[State 200, Move Forward] ; Causes the character to move forward slightly on keyframe 2 of the animation
type = VelSet
trigger1 = animelem = 2
x = 3

[State 200, Attack!] ;This is an Example Hitdef.
type = HitDef
getpower = 0 ;Makes sure the character doesnt get power, as there is seperate power gain code already.
triggerall = map(juggle) < 100 ; If the move hasn't hit juggle limit (Custom juggle system)
triggerall = !movecontact ;If the move hasn't made contact...
trigger1 = 1
attr = S, NA ;Attribute of this attack is a Standing, Normal Attack.
animtype  = Light ;Opponent will go into a light hit animation if hit.
damage    = 10 ;Does 10 damage. KBB Characters commonly have 1000 Health Points at the max. 2000 if a boss.
hitflag = MAF ;Hits Mid, Air, Falling and Downed Opponents.
guardflag = M ;Can only be blocked on the ground, in all block Directions. Parameters allowed are (L,M,H) - Choose One), and A (Air.).
pausetime = 10,10 ; First value is pause on Player 1, Second value is Pause on Player 2.
sparkno = 0 ;Valid Hitsparks are 0 (Light), 1 (Medium), or 2(Heavy). You can also just add your own custom one within Sprites and Animations.
guard.sparkno = 13000 ;Guard Hitspark is always this unless you have a custom one. If you dont wanna specify a hitspark either erase it or make the number -1.
sparkxy = -8,-25 ;Location the hitspark spawns in. Calculates from Player 1 (You)'s Position.
hitsound   = 5,1 ;Plays the Following Hitsound. Adding S will make it play from the player's sound files.
guardsound = 6,10 ;Plays the Following Guard Sound. Takes from common.snd, just like hitsound. To use sounds from here, open that up and take a listen to every sound.
ground.type = High ;Always leave this at High. KBB Characters have no Low because they dont have proper sprites in the 5000 Area.
ground.slidetime = 12 ;How long the character will slide on the ground. This is not related to sliding knockdown.
ground.hittime  = 10 + ifelse(enemy,movetype = A, 7,0) ;How long the opponent stays in hitstun if NOT in a Juggle State. The code that adds 7 is for if you get a normal Counterhit.
ground.velocity = 1.5,0 ;Velocities. This is in the style of an inverted graph unless you are working with wallbounce state.
air.velocity = 2,-1 ;See Above.
Fall = 0 ;Specifies if the Attack should put the opponent in juggle state if not already in juggle state.
fall.recover = 1
Fall.recovertime = 20 + ifelse(enemy,movetype = A, 7,0) ;How long the opponent stays in hitstun if IN a Juggle State. If the attack causes fall, adds an additional 7 frames on Counterhit.
guard.cornerpush.veloff = -5 ;Velocity the character pushes themself away if too close to the edge of the state (outright touching) IF the opponent blocked the attack.
airguard.cornerpush.veloff = -4 ;Velocity the character pushes themself away if too close to the edge of the state (outright touching) IF the opponent blocked the attack IN THE AIR.

[State 200, Wavedash Cancel]
type = ChangeState
Triggerall = command = "WD" && command != "NotWD"
trigger1 = movehit || (movecontact && AnimTime > -7)
value = 30
ctrl = 1

[State 200, Cancel into Low Attack] ; Allows this move to cancel into a low attack
type = ChangeState
Triggerall = movecontact ; If the move made contact (Hit the opponent OR if it was blocked)
trigger1 = command = "holddown" ; AND the player is holding down
trigger1 = command = "a" ; AND the player presses "a"
value = 950 ; Go to this state (Low attack)

[State 200, Cancel into Down C]
type = ChangeState
Triggerall = movecontact ; If the move made contact
Triggerall = power >= 200 ; AND the character has at least 200 power (1/5th of a bar)
triggerall = numhelper(3500) = 0 ; AND the character is not glitched
trigger1 = command = "holddown" ; AND the player is holding down
trigger1 = command = "c" ; AND the player presses c
value = 450 ; Go to this state (Down + C attack)

[State 220, Cancel into 6C]
type = ChangeState
triggerall = movecontact
triggerall = power >= 200
triggerall = numhelper(3500) = 0
trigger1 = command = "c" && command = "holdfwd"
value = 460

[State 200, Cancel into AA]
type = ChangeState
Triggerall = movecontact
trigger1 = command = "a"
trigger2 = roundstate = 2 && ailevel > 0 && random < (ailevel * 100)
value = 210

[State 200, Cancel into B]
type = ChangeState
Triggerall = movecontact
trigger1 = command = "b"
trigger2 = roundstate = 2 && ailevel > 0 && random < (ailevel * 100)
value = 300

[State 220, Cancel into 5C]
type = ChangeState
triggerall = movecontact
triggerall = power >= 200
triggerall = numhelper(3500) = 0
trigger1 = command = "c"
value = 400

[State 200, End]
type = ChangeState
trigger1 = AnimTime = 0 ; When the animation ends
value = 0 ; Go to this state (Standing)
ctrl = 1 ; Gives the player control when in the standing state
