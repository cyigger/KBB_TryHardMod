f; Definition file for player
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Bubbla"        ;Name of character
displayname = "Bubbla" ;Name of character to display
versiondate = 1/18/2024   ;Version date of character
mugenversion =  1.1 ;Version of M.U.G.E.N character works on
ikemenVersion = 0.98
author = "KBB Dev Team + thePlantalagist"      ;Character author name
pal.defaults = 1,2,3,4,5,6,7,8  ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = Bubbla.cmd   ;Command set
cns     = Bubbla.cns   ;Constants
st      = Bubbla.cns   ;States
st1     = Specials.cns
;st2     = StarBurst.cns
;st3     = Power Delay.cns
;st4     = StarStrike.cns
;st5     = HurtStates.cns
stcommon = common1.cns ;Common states
sprite  = Bubbla.sff   ;Sprite
anim    = Bubbla.air   ;Animation
sound  = Bubbla.snd   ;Sound
movelist = movelist.dat

[Palette Keymap]
a = 1
b = 1
c = 1
x = 1
y = 1
z = 1