; var(3):= 45 - AirJump Start, -3
; var(4):= 66 - Instant Airdash:, -3
;var(11):= 50000 - Star Burst, 51000 - Star Burst Start, 54000 - Star Buster, 54500 - Star Burst (In Neutral & off Block), 51001 - Star Burst ground dash proration cooldown, 51002 - Star Burst air dash proration cooldown, -3 - StarBurst Negative Statedef Stuff, 195 - Palette Selection, 220 - A - 3, 300 - B:, 310 - B - 2, 400 - C - 1:, 420 - C - 3:, 465 - Fwd+C, 700 - Grab, 900 - Overhead, 905 - Overhead Charged, 950 - Low Attack, 600 - A - Aire, 610 - B - Aire, 615 - C - Air Ammoless, 620 - C - Aire, 635 - Down C - Aire 2, 630 - Down C - Aire, -3, 22500 - Dunk Slam Success
;var(29):= -3 - Dont mess with; Executed when in self state
;var(30):= -3
;var(41):= -3
;var(47):= 400 - C - 1:, 410 - C - 2:, 420 - C - 3:, 465 - Fwd+C, 450 - Down C - Reload:, 700 - Grab, 905 - Overhead Charged, 610 - B - Aire, 620 - C - Aire, 625 - C - Aire 2, 635 - Down C - Aire 2, 630 - Down C - Aire, -3, 22500 - Dunk Slam Success, 22550 - Dunk Slam Follow, 22560 - Dunk Slam Final Shot, 23000 - Shooty Shooty - AIR, 23100 - Shooty Shooty - STANDING, 23500 - Shooty Shooty Success, 24000 - Quintuple Geyser, 25000 - lmao
