; Definition file for player
; Contains all the filenames needed for the character

; Player information
[Info]
name = "MetaKnight"        ;Name of character
displayname = "MetaKnight" ;Name of character to display
versiondate =10/25/2020    ;Version date of character
mugenversion =  1.1 ;Version of M.U.G.E.N character works on
ikemenVersion = 0.98
author = "KBB Dev Team & JonTheCopycat"      ;Character author name
pal.defaults = 1,2,3,4,5,6  ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = KBB MK.cmd   ;Command set
cns     = KBB MK.cns   ;Constants
st      = KBB MK.cns   ;States
st1     = Specials.cns
;st2     = HurtStates.cns
stcommon = common1.cns ;Common states
sprite  = KBB MK.sff   ;Sprite
anim    = KBB MK.air   ;Animation
sound  = KBB MK.snd   ;Sound
movelist = movelist.dat

[Palette Keymap]
a = 1
b = 1
c = 1
x = 1
y = 1
z = 1