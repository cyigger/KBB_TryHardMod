 ; Definition file for player
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Yoru"        ;Name of character
displayname = "Yoru" ;Name of character to display
versiondate = 3/12/2022   ;Version date of character
mugenversion = 1.1  ;Version of M.U.G.E.N character works on
ikemenVersion = 0.98
author = "KBB Dev Team"      ;Character author name
pal.defaults = 1,2,3,4,5,6,7,8  ;Default palettes in order of preference

; Files for the player
[Files]
cmd     = Yoru.cmd   ;Command set
cns     = Yoru.cns   ;Constants
st      = Yoru.cns   ;States
st1     = Specials.cns
;st2     = StarBurst.cns
;st3     = Power Delay.cns
;st4     = StarStrike.cns
;st5     = HurtStates.cns
stcommon = common1.cns ;Common states
sprite  = Yoru.sff   ;Sprite
anim    = Yoru.air   ;Animation
sound  = Yoru.snd   ;Sound
movelist = movelist.dat

[Palette Keymap]
a = 1
b = 1
c = 1
x = 1
y = 1
z = 1