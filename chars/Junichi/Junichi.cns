[State -2, EnableTag] ; Renables tagging in
type = MapSet
triggerall = teammode = tag
trigger1 = command != "SPECIAL 5" && command != "SPECIAL 2" && command != "SPECIAL 3" && command != "SPECIAL 4" && command != "SPECIAL 5" && command != "SPECIAL 6"
map = "disableTag"
value = 0

[State -2, DisableTagBuffer] ; If the player buffers a special move, disables tagging
